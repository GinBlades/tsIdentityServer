{
  "name": "tsidentityserver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/bin/www.js",
    "watch-node": "nodemon ./dist/bin/www.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"npm run watch-node\" \"npm run watch-ts\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.3.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "mongoose": "^5.1.6",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/connect-flash": "0.0.34",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/http-errors": "^1.6.1",
    "@types/mongoose": "^5.0.16",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.3.4",
    "@types/node-sass-middleware": "0.0.30",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "concurrently": "^3.6.0",
    "nodemon": "^1.17.5",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
