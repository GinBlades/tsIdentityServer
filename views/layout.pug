doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title Identity Server
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    .container
      if hasFlash
        ul.flash-container
          each val, key in flash
            li= key + ": " + val

      block content
